{
    "name": "BusinessPartnerVerification",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "type": "module",
    "dependencies": {
        "@sap-cloud-sdk/core": "canary",
        "@sap/cds": "^4.5.1",
        "@sap/cloud-sdk-vdm-business-partner-service": "^1.24.0",
        "@sap/hana-client": "^2.7.23",
        "@sap/xb-msg-amqp-v100": "^0.9.48",
        "@sap/xsenv": "^2.2.0",
        "@sap/xssec": "^2.2.5",
        "express": "^4",
        "hdb": "^0.18.3",
        "lodash": "^4.17.20",
        "passport": "^0.4.1"
    },
    "devDependencies": {
        "@sap/approuter": "9.0.x",
        "@sap/eslint-plugin-ui5-jsdocs": "2.0.x",
        "@sap/hdi-deploy": "3.11.15",
        "@sap/html5-repo-mock": "2.0.x",
        "@sap/ui5-builder-webide-extension": "1.0.x",
        "@sap/ux-specification": "^1.84.2",
        "@sapui5/ts-types": "1.71.x",
        "@ui5/cli": "2.2.6",
        "bestzip": "2.1.7",
        "eslint": "5.16.x",
        "rimraf": "3.0.2",
        "sqlite3": "5.0.0"
    },
    "scripts": {
        "start": "npx -n=\"--inspect\" cds run"
    },
    "cds": {
        "hana": {
            "deploy-format": "hdbtable"
        },
        "requires": {
            "db": {
                "kind": "sql"
            },
            "messaging": {
                "kind": "file-based-messaging",
                "[production]": {
                    "kind": "enterprise-messaging-shared"
                },
                "file": "../msg-box"
            },
            "OP_API_BUSINESS_PARTNER_SRV": {
                "kind": "odata",
                "model": "srv/external/OP_API_BUSINESS_PARTNER_SRV",
                "credentials": {
                    "destination": "BusinessPartner",
                    "path": "/sap/opu/odata/sap/API_BUSINESS_PARTNER",
                    "requestTimeout": 1000000
                }
            }
        }
    },
    "sapux": [
        "app/businesspartners"
    ],
    "ui5": {
        "dependencies": [
            "@sap/ui5-builder-webide-extension"
        ]
    }
}
